<%= form_tag action: :authentication, method: :post do %>
 <table class="table table-striped">
 <tr>
 <td><%= label_tag :email%></td>
 <td><%= email_field_tag :email%></td>
 </tr>
  <tr>
 <td><%= label_tag :password%></td>
 <td><%= password_field_tag :password%></td>
 </tr>
 <tr>
 <td><%= submit_tag "Sign In"%></td>
  <td><%= link_to "Sign Up", new_user_path%></td>
 </tr>
 </table>
<% end %>
