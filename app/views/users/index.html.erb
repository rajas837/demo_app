<br>
<br>
<%= hello(return_user_id)%> 
<%= image_tag profile_pic if profile_pic.present?%>
<table class="table table-striped">
<tr>
    <th>Profile Pic</th>
    <th>Name</th>
    <th>Email</th>
    <th>Mobile No</th>
    <th>Address</th>
    <th><%= link_to "Download into CSV", download_in_csv_users_path%></th>
</tr>
<% @users.each do | user | %>
    <tr>
        <td><%= image_tag user.image.photo(:thumb) unless user.image.nil? %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.mobile_no %></td>
        <td><%= user.address %></td>
        <td><%= link_to "Show", user_path(user) %></td>
        <td><%= link_to "Edit", edit_user_path(user) %></td>
        <td><%= link_to "Delete", user, method: :delete, data: {confirm: "Are you sure ?"} %></td>
    </tr>
<%end%>
</table>
<%= link_to "Add users",new_user_path %><br/>
<%= link_to "Go to Products",products_path %>